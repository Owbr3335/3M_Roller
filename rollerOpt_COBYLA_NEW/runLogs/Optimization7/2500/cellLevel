FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2500";
    object      cellLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lowerOutlet
    {
        type            calculated;
        value           uniform 0;
    }
    top
    {
        type            calculated;
        value           uniform 0;
    }
    sides
    {
        type            calculated;
        value           uniform 0;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    roller
    {
        type            calculated;
        value           uniform 0;
    }
    burner
    {
        type            calculated;
        value           uniform 0;
    }
}


